{
  "functions": {
    "api/bypass.js": {
      "maxDuration": 60,
      "memory": 3008
    }
  },
  "routes": [
    {
      "src": "/api/bypass",
      "methods": ["GET", "POST"],
      "dest": "/api/bypass"
    },
    {
      "src": "/",
      "dest": "/api/bypass"
    },
    {
      "src": "/docs",
      "dest": "/api/bypass"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
